{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "Earthquake-gptl"
		},
		"ds_RestService_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://earthquake.usgs.gov/earthquakes/feed/v1.0/summary"
		},
		"ds_azureDataLake_daily_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://earthquakegptl.dfs.core.windows.net/"
		},
		"ds_azure_daily_data_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://gptlearthquake.dfs.core.windows.net/"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "Earthquake-gptl_ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2024-03-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"ds_RestService_properties_typeProperties_url": {
						"value": "[parameters('ds_RestService_properties_typeProperties_url')]"
					},
					"ds_azureDataLake_daily_properties_typeProperties_url": {
						"value": "[parameters('ds_azureDataLake_daily_properties_typeProperties_url')]"
					},
					"ds_azure_daily_data_properties_typeProperties_url": {
						"value": "[parameters('ds_azure_daily_data_properties_typeProperties_url')]"
					}
				}
			},
			"dependsOn": []
		}
	]
}